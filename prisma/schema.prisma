

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Message {
  id String @id @default(auto()) @map("_id") @db.ObjectId 
  name String 
  email String 
  message String
  @@unique([email])
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  password String
}